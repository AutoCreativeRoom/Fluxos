lector('#__next');\n        if (elementoPai) {\n          elementoPai.insertBefore(userBar, elementoPai.firstChild);\n        }\n      }\n    \n      criarBarra();\n    })();\n  </script>\n  <script>\n    const botBody = document.querySelector('typebot-standard')\n      .shadowRoot.querySelector('.typebot-container');\n    const userAvatar = botBody.querySelector('img[elementtiming=\"Bot avatar\"]');\n    const status = document.querySelector('.status');\n  \n    // Classes constantes\n    const bubbleClass = '.bubble1';\n    const statusClass = '.status';\n    const mediaObjectClass = '.media-object';\n  \n    createInfoMessage();\n  \n    function createInfoMessage() {\n  // URLs e informa√ß√µes\n  const supportURL = 'https://seguro.afcode.store/r/INQ0WFFVL4';\n  const supportImageURL = 'https://i.postimg.cc/fT27f1cV/download.png';\n  const supportText = 'D√∫vidas? Fale conosco!';\n  const responseTime = 'üü¢ Costuma responder em at√© 20 minutos';\n  const supportPlayPixURL = 'https://seguro.afcode.store/r/INQ0WFFVL4';\n  const minhaDivURL = 'https://seguro.afcode.store/r/INQ0WFFVL4';\n  const minhaDivImageURL = 'https://i.postimg.cc/jqH1xqzy/Banner-2-2-1024x374.webp';\n\n  const div = document.createElement('div');\n  const infoHTML = `\n    <div>\n      <div class=\"user-metadata\">\n        <div class=\"avatar\">\n          <div>\n            <a href=\"${supportURL}\">\n              <img src=\"${supportImageURL}\" width=\"30%\" alt=\"Minha Imagem\" class=\"${mediaObjectClass}\">\n            </a>\n            <div class=\"primary-info\">\n              <p class=\"name\">${supportText}</p>\n              <p>${responseTime}</p>\n            </div>\n            <a href=\"${supportPlayPixURL}\" target=\"_blank\">Pulse Solution</a>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div id=\"minhaDiv\">\n      <a href=\"${minhaDivURL}\">\n        <img src=\"${minhaDivImageURL}\" alt=\"Minha Imagem\" class=\"${mediaObjectClass}\">\n      </a>\n    </div>`;\n\n  div.innerHTML = infoHTML;\n  const messagesContainer = botBody.querySelector('.typebot-chat-view');\n  messagesContainer.insertBefore(div, messagesContainer.firstChild);\n}\n\n  \n    setInterval(() => {\n      const isTyping = botBody.querySelector(bubbleClass);\n      const sibling = isTyping?.parentElement?.parentElement?.nextSibling;\n  \n      if (isTyping && sibling.src) {\n        status.innerText = 'gravando audio...';\n      } else if (isTyping) {\n        status.innerText = 'digitando...';\n      } else {\n        status.innerText = 'Suporte';\n      }\n  \n      const allMessages = botBody.querySelector('.typebot-chat-view').querySelectorAll('.items-start.typebot-host-bubble');\n    }, 400);\n  </script>\n"}},"createdAt":"2023-11-03T14:06:10.605Z","updatedAt":"2023-11-03T14:43:03.154Z","icon":"https://i.postimg.cc/9Q0Vwwp6/download-copiar.png","folderId":"clnuevu26003oqi1pwgyjpti7","publicId":"suporte-dh9nkvu","customDomain":null,"workspaceId":"clnr64nj00001n21oaw5nat15","resultsTablePreferences":null,"isArchived":false,"isClosed":false,"whatsAppCredentialsId":null}